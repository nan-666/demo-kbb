/* pages/search/search.wxss */
.container{
  height: 100vh;
  padding: 32rpx;
  background-color: #ededed;
}
.container .search-bar{
  display: flex;
  align-items: center;
  background-color: #fff;
  border-radius: 32rpx;
  padding: 10rpx 32rpx;
  font-size: 28rpx;
}
.container .search-bar ._input{
  width: 100%;
}
.container .search-bar ._img{
  width: 28rpx;
  height: 28rpx;
  margin-right: 20rpx;
}
.container ._button{
  background-color: #006fff;
  color: #fff;
  width: 80vw;
  position: absolute;
  left: 50%;
  bottom: 48rpx;
  margin-left: -40vw;
}
.container .clicked{
  background-color: #0063e5;
}