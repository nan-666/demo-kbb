<!--pages/detail/detail.wxml-->
<view class="head">
  <view class="head-word">
    <text>交易成功</text>
  </view>
</view>
<view class="middle" >
  <view  wx:for="{{service}}" wx:key="index" data-index="{{index}}">
  <view class="service">
    <view class="service-img">
      <image src="{{item.img_1}}"/>
    </view>
    <view class="service-name">{{item.type}}</view>
    <view class="service-money">¥{{item.money}}</view>
    <view class="service-detail">{{item.title}}</view>
    <view class="service-num">×1</view>
  </view>
  <view class="return">退换</view>
  <view class="price">
    <view class="price-name">商品总价</view>
    <view class="price-num">¥{{item.money}}</view>
    <view class="price-name">实付款</view>
    <view class="price-num">¥{{item.money}}</view>
  </view>
  </view>
</view>
